<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:react="http://www.jahia.org/jahia/react/1.0">
    <modules jcr:primaryType="jnt:modules">
        <react-dxcomponents j:dependencies="default"
                          j:modulePriority="0"
                          j:moduleType="module"
                          j:title="react-dxcomponents"
                          jcr:mixinTypes="jmix:hasExternalProviderExtension"
                          jcr:primaryType="jnt:module">
                <sources jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                  jcr:primaryType="jnt:moduleVersionFolder"
                  j:staticMountPointProviderKey="module-react-dxcomponents-1.0-SNAPSHOT">
                    <src jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                 jcr:primaryType="jnt:folder">
                        <main jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                     jcr:primaryType="jnt:folder">
                            <import jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                          jcr:primaryType="jnt:folder">
                                <repository.xml jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:xmlFile"/>
                            </import>
                        </main>
                    </src>
                </sources>
                
                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder">
                </contents>
                <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
                    <files jcr:primaryType="jnt:folder"/>
                    <contents jcr:primaryType="jnt:contentFolder">
                        <examples jcr:primaryType="jnt:contentFolder">
                            <j:translation_en jcr:language="en"
                                              jcr:mixinTypes="mix:title"
                                              jcr:primaryType="jnt:translation"
                                              jcr:title="examples"/>
                            <layout jcr:primaryType="reactnt:layout">
                                <leftCol jcr:primaryType="reactnt:contentPicker"
                                         rootPath="/sites/digitall"
                                         storeId="main"/>
                                <rightCol jcr:primaryType="reactnt:contentTable" storeId="main"/>
                            </layout>

                            <action-bar jcr:primaryType="reactnt:actionBar" react:id="main"/>
                        </examples>

                        <action-bars jcr:primaryType="jnt:contentFolder">
                            <main jcr:primaryType="reactnt:componentsFolder" react:id="main">
                                <simple-button-1 jcr:primaryType="reactnt:actionUrl" url="http://www.jahia.com" icon="Accessibility"/>
                                <simple-button-2 jcr:primaryType="reactnt:actionUrl" url="http://www.jahia.com" icon="Autorenew"/>
                            </main>
                        </action-bars>
                    </contents>
                    <base j:hiddenTemplate="false" jcr:primaryType="jnt:template">
                        <j:translation_en jcr:language="en"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="base"/>
                        <react-template j:applyOn="reactmix:reactComponent"
                               j:defaultTemplate="true"
                               j:hiddenTemplate="true"
                               jcr:primaryType="jnt:contentTemplate">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="react-template"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <main-resource-display jcr:primaryType="jnt:mainResourceDisplay"/>
                            </pagecontent>
                        </react-template>
                    </base>
                </templates>
                
            </react-dxcomponents>
    </modules>
</content>