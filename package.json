{
  "name": "@jahia/react-dxcomponents",
  "version": "1.0.0",
  "scripts": {
    "babel": "cross-env NODE_ENV=production babel ./src/javascript --ignore *.test.js --out-dir ./build",
    "babel:es": "cross-env NODE_ENV=production BABEL_ENV=es babel ./src/javascript --ignore *.test.js --out-dir ./build/es",
    "react2dts": "cross-env NODE_ENV=production babel-node react2dts.js",
    "prepare-build": "cross-env NODE_ENV=production babel-node prepare-build.js",
    "webpack": "webpack --config webpack-umd.config.js",
    "rollup": "rollup -c",
    "uglify": "uglifyjs -c -m --source-map 'content=build/lib/react-dxcomponents.umd.js.map' -o build/lib/react-dxcomponents.umd.min.js  -- build/lib/react-dxcomponents.umd.js",
    "dist": "yarn run rollup; yarn run uglify",
    "js-bundle-dxcomponent": "webpack",
    "js-bundle-vendors": "webpack --config webpack-vendors.config.js",
    "build-old": "yarn run dist && yarn run js-bundle-dxcomponent && yarn run js-bundle-vendors",
    "build": "yarn run babel && yarn run prepare-build && yarn run dist",
    "publish-script": "node publish.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/history": "^4.6.2",
    "@types/i18next": "^8.4.3",
    "@types/react": "^16.3.14",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-react-remove-properties": "^0.2.5",
    "babel-plugin-transform-dev-warning": "^0.1.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "postcss-cssnext": "^3.0.2",
    "postcss-each": "^0.10.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-mixins": "^6.1.0",
    "react-to-typescript-definitions": "^1.0.0",
    "rollup": "^0.59.2",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "style-loader": "^0.18.2",
    "uglify-js": "^3.3.4",
    "webpack": "^4.7.0",
    "webpack-cli": "^2.1.2",
    "webpack-node-externals": "^1.6.0",
    "webpack-systemjs-bundle-plugin": "https://github.com/Jahia/webpack-systemjs-bundle-plugin.git",
    "yarn-api": "^1.1.0"
  },
  "dependencies": {
    "@jahia/apollo-dx": "git+ssh://git@github.com:Jahia/apollo-dx.git",
    "apollo-client": "^2.2.8",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.1",
    "history": "^4.7.2",
    "i18next": "^10.3.0",
    "i18next-chained-backend": "^0.1.2",
    "i18next-xhr-backend": "^1.5.1",
    "jss": "^9.4.0",
    "jss-preset-default": "^4.0.1",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.36",
    "material-ui-icons": "^1.0.0-beta.17",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-apollo": "^2.1.3",
    "react-dom": "^16.2.0",
    "react-flexbox-grid": "^2.0.0",
    "react-i18next": "^7.3.1",
    "react-jss": "^8.1.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-extend-reducer": "^0.2.1",
    "theming": "^1.2.1"
  }
}
